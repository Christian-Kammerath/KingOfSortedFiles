<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="KingOfSortedFiles.Views.SettingWindow"
        Title="SettingWindow">
        <StackPanel HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                <TextBox x:Name="TextBox" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" TextWrapping="Wrap" AcceptsReturn="True"></TextBox>
                <Button Content="Save" Background="Green" Click="SaveButton_OnClick" HorizontalAlignment="Right" VerticalAlignment="Bottom"></Button>
        </StackPanel>
        
        
</Window>
